{
    "name": "myparcelcom/integration-commons",
    "description": "Library with common classes used for constructing MyParcel.com eCommerce Integrations",
    "type": "library",
    "require": {
        "php": "^8.0",
        "ramsey/uuid": "^4.1",
        "myclabs/php-enum": "^1.8",
        "laravel/framework": "^8.34"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "roave/security-advisories": "dev-latest",
        "fakerphp/faker": "^1.13"
    },
    "autoload": {
        "psr-4": {
            "MyParcelCom\\Integration\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Yoan-Alexander Grigorov",
            "email": "yoan@myparcel.com"
        }
    ],
    "minimum-stability": "stable"
}
